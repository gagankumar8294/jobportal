<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link rel="stylesheet" href="style.css">
  </head>
  <body>
    <nav class="navbar">
            <a href="#">Home</a>
            <a href="#">Jobs</a>
            <a href="#">Recruiter</a>
    </nav>
    <tbody>
        <% applicants.forEach(applicant => { %>
        <tr>
            <th><%= applicant.id %></th>
            <td><%= applicant.name %></td>
            <td><%= applicant.description %></td>
            <td><%= applicant.price %></td>
            <td>
                <img src="<%= applicant.imageUrl %>">
            </td>
        </tr>
        <% }) %>
    </tbody>
    
  </body>
</html>